{
  "name": "france-topojson",
  "version": "1.0.0",
  "description": "Générateur de fichiers TopoJSON optimisés pour les limites administratives françaises",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "download": "node scripts/download.js",
    "convert": "node scripts/convert.js",
    "build": "npm run download && npm run convert",
    "build:regions": "node scripts/convert.js --layer=regions",
    "build:departements": "node scripts/convert.js --layer=departements",
    "build:epci": "node scripts/convert.js --layer=epci",
    "build:communes": "node scripts/convert.js --layer=communes",
    "clean": "rm -rf data/sources/* data/geojson/* data/topojson/*"
  },
  "keywords": [
    "france",
    "topojson",
    "geojson",
    "boundaries",
    "administrative",
    "regions",
    "departements",
    "epci",
    "ign"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "7zip-bin": "^5.1.1",
    "mapshaper": "^0.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
